              .four-delivery-charge__1
                .four-delivery-charge__2
                  .four-delivery-charge-box
                    %label 配送料の負担
                    %span.four-delivery-charge-box-1 必須
                .four-choice
                  .four-choice__1
                    %select{name: "four-choice__2", style: "width: 620px; height: 30px;"}
                      %option{"aria-selected": "true", label: "選択してください", selected: "selected", value: ""} 選択してください
                      %option{"aria-selected": "false", label: "送料込み(出品者負担)", value: "2"} 送料込み(出品者負担)
                      %option{"aria-selected": "false", label: "着払い(購入者負担)", value: "1"} 着払い(購入者負担)
                      %svg{"aria-hidden": "true", fill: "#888888", "fill-rule": "evenodd", height: "24", viewbox: "0 0 24 24", width: "24"}
                        %path{d: "M12,15.66a1.73,1.73,0,0,1-1.2-.49L5.21,9.54a.7.7,0,0,1,1-1l5.62,5.62c.15.15.27.15.41,0L17.8,8.6a.71.71,0,0,1,1,0,.69.69,0,0,1,0,1l-5.57,5.58A1.71,1.71,0,0,1,12,15.66Z"}
            .four-area
              .four-area-in
                %div{name: "area-box"}
                  .area-box__1
                    %label 発送元の地域
                    %span.area-box__2 必須
                .four1-area-choice
                  .four1-area-choice__1
                    %select{name: "four1-area-choice__2", style: "width: 620px; height: 30px;"}
                      %option{"aria-selected": "true", label: "選択してください", selected: "selected", value: ""} 選択してください
                      %option{"aria-selected": "false", label: "北海道", value: "1"} 北海道
                      %svg{"aria-hidden": "true", fill: "#888888", "fill-rule": "evenodd", height: "24", viewbox: "0 0 24 24", width: "24"}
                        %path{d: "M12,15.66a1.73,1.73,0,0,1-1.2-.49L5.21,9.54a.7.7,0,0,1,1-1l5.62,5.62c.15.15.27.15.41,0L17.8,8.6a.71.71,0,0,1,1,0,.69.69,0,0,1,0,1l-5.57,5.58A1.71,1.71,0,0,1,12,15.66Z"}
            .four-shipping-days
              .four-shipping-days-1
                .four-shipping-days-2
                  .four-shipping-days-box
                    %label 発送までの日数
                    %span.four-shipping-days-box__1 必須
                .four-shipping-days-choice
                  .four-shipping-days-choice__1
                    %select{name: "four-shipping-days-choice__2", style: "width: 620px; height: 30px;"}
                      %option{"aria-selected": "true", label: "選択してください", selected: "selected", value: ""} 選択してください
                      %svg{"aria-hidden": "true", fill: "#888888", "fill-rule": "evenodd", height: "24", viewbox: "0 0 24 24", width: "24"}
                        %path{d: "M12,15.66a1.73,1.73,0,0,1-1.2-.49L5.21,9.54a.7.7,0,0,1,1-1l5.62,5.62c.15.15.27.15.41,0L17.8,8.6a.71.71,0,0,1,1,0,.69.69,0,0,1,0,1l-5.57,5.58A1.71,1.71,0,0,1,12,15.66Z"}
          %hr.fiveline/
          .fivebox-all
            %p.five-pricebox
              価格 (¥300
              / 
              / 
              \~
              
              9,999,999
              / 
              )
              %a{href: "/jp/help_center/article/64/", rel: "noopener noreferrer", target: "_blank"}
                %svg.five-hatena{"aria-hidden": "true", fill: "#222222", "fill-rule": "evenodd", height: "24", viewbox: "0 0 24 24", width: "24"}
                  %path{d: "M12,2A10,10,0,1,0,22,12,10,10,0,0,0,12,2Zm0,18.6A8.6,8.6,0,1,1,20.6,12,8.61,8.61,0,0,1,12,20.6Z"}
